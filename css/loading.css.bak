
    @charset "UTF-8";
    .mots_loader {
      width: 595px !important;
      height: 100% !important;
      position: relative;
	  background-color:#2d4654;
    }
    .mots_loader::before, .mots_loader::after {
      content: "";
      width: 0;
      height: 0;
      position: absolute;
      bottom: 30px;
      left: 15px;
      z-index: 1;
      border-left: 50px solid transparent;
      border-right: 50px solid transparent;
      border-bottom: 20px solid #1b2a33;
      transform: scale(0);
      transition: all 0.2s ease;
    }
    .mots_loader::after {
      border-right: 15px solid transparent;
      border-bottom: 20px solid #162229;
    }
    .mots_loader .getting-there {
      width: 100%;
      text-align: center;
      position: absolute;
      bottom: 0;
      left: 0;
      font-size: 18px;
      letter-spacing: 2px;
      color: white;
    }
    .mots_loader .binary {
      width: 50%;
      height: 97% !important;
      display: block;
      color: white;
      position: absolute;
      top: 0;
      left: 220px;
      z-index: 2;
      overflow: hidden;
    }
    .mots_loader .binary::before, .mots_loader .binary::after {
      font-family: "M PLUS 1", sans-serif;
      font-size: 24px;
      position: absolute;
      top: 0;
      left: 0;
      opacity: 0;
    }
    .mots_loader .binary:nth-child(1)::before {
      content: "Québec";
      -webkit-animation: a 6.6s linear infinite;
              animation: a 6.6s linear infinite;
    }
    .mots_loader .binary:nth-child(1)::after {
      content: "Pensée";
      -webkit-animation: b 5.1s linear infinite;
              animation: b 5.1s linear infinite;
    }
    .mots_loader .binary:nth-child(2)::before {
      content: "Libre";
      -webkit-animation: c 4.9s linear infinite;
              animation: c 4.9s linear infinite;
    }
    .mots_loader .binary:nth-child(2)::after {
      content: "Efforts";
      -webkit-animation: d 5.7s linear infinite;
              animation: d 5.7s linear infinite;
    }
    .mots_loader .binary:nth-child(3)::before {
      content: "Allo";
      -webkit-animation: e 5.9s linear infinite;
              animation: e 5.9s linear infinite;
    }
    .mots_loader .binary:nth-child(3)::after {
      content: "Bonjour";
      -webkit-animation: f 6.2s linear infinite;
              animation: f 6.2s linear infinite;
    }
    .mots_loader .binary:nth-child(4)::before {
      content: "Salut";
      -webkit-animation: g 3.9s linear infinite;
              animation: g 3.9s linear infinite;
    }
    .mots_loader .binary:nth-child(4)::after {
      content: "France";
      -webkit-animation: h 4.7s linear infinite;
              animation: h 4.7s linear infinite;
    }
    .mots_loader .binary:nth-child(5)::before {
      content: "Paris";
      -webkit-animation: i 7.2s linear infinite;
              animation: i 7.2s linear infinite;
    }
    .mots_loader .binary:nth-child(5)::after {
      content: "Montréal";
      -webkit-animation: j 6.5s linear infinite;
              animation: j 6.5s linear infinite;
    }
    .mots_loader .binary:nth-child(6)::before {
      content: "Monde";
      -webkit-animation: k 4.9s linear infinite;
              animation: k 4.9s linear infinite;
    }
    .mots_loader .binary:nth-child(6)::after {
      content: "Français";
      -webkit-animation: l 4.1s linear infinite;
              animation: l 4.1s linear infinite;
    }
    .mots_loader .binary:nth-child(7)::before {
      content: "2022";
      -webkit-animation: m 7.3s linear infinite;
              animation: m 7.3s linear infinite;
    }
    .mots_loader .binary:nth-child(7)::after {
      content: "Vive la Liberté!";
      -webkit-animation: n 7.4s linear infinite;
              animation: n 7.4s linear infinite;
    }
    .mots_loader .binary:nth-child(8)::before {
      content: "Vive l'égalité!";
      -webkit-animation: o 5.9s linear infinite;
              animation: o 5.9s linear infinite;
    }
    .mots_loader .binary:nth-child(8)::after {
      content: "Vive la Fraternité!";
      -webkit-animation: p 7.7s linear infinite;
              animation: p 7.7s linear infinite;
    }
    .mots_loader.mots_js_on::before, .mots_loader.mots_js_on::after {
      transform: scale(1);
	  left:40%;
    }
    
    @-webkit-keyframes a {
      0% {
        transform: translate(30px, 0) rotate(30deg);
        opacity: 0;
      }
      100% {
        transform: translate(30px, 850px) rotate(-50deg);
        opacity: 1;
      }
    }
    
